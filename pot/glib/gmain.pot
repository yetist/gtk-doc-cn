#. used by: SECTION:main
#: gmain.c:113:1
msgid "@title: The Main Event Loop"
msgstr ""

#. used by: SECTION:main
#: gmain.c:114:1
msgid "@short_description: manages all available sources of events"
msgstr ""

#. used by: g_source_remove_poll:
#: gmain.c:1148:1
