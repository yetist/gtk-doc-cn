msgid "@end: (nullable): a pointer to the byte following the end of the string, or %NULL to indicate that the string is nul-terminated"
msgstr ""

#. used by: g_utf8_find_next_char:
#: gutf8.c:159:1
msgid "Finds the start of the next UTF-8 character in the string after @p."
msgstr ""

#. used by: g_utf8_validate:
#: gutf8.c:1642:1
msgid "@str: (array length=max_len) (element-type guint8): a pointer to character data"
msgstr ""

# used by: g_utf8_validate:
