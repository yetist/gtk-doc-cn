#: gresolver.c:407:2
msgid ""
"Synchronously reverse-resolves @address to determine its associated hostname."
msgstr ""

#. #-#-#-#-#  gresolver.pot~ (PACKAGE VERSION)  #-#-#-#-#
#. used by: g_resolver_lookup_by_address:
#. #-#-#-#-#  gresolver.pot~ (PACKAGE VERSION)  #-#-#-#-#
#. used by: g_resolver_lookup_by_address_finish:
#: gresolver.c:417:2 gresolver.c:478:2
